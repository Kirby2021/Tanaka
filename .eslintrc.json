{
	"extends": "aqua/node",
	"rules": {
		"comma-dangle": "off",
		"arrow-parens": "off",
		"quote-props": "off",
		"no-mixed-operators": "off",
		"indent": "off",
		"newline-per-chained-call": "off",
		"radix": "off",
		"curly": "off",
		"no-extra-parens": "off",
		"function-paren-newline": "off",
		"arrow-body-style": "off"
	}
}
